@model IEnumerable<Pickup_Entity.Product>

@{
    if (Session["USER"]!=null)
    {
        ViewBag.Title = "Home";
        Layout = "~/Views/Shared/_LayoutUser.cshtml";
    }

    else
    {
        ViewBag.Title = "Home";
        Layout = "~/Views/Shared/_LayoutHome.cshtml";
    }

}

    @foreach (var item in Model)
    {

        @Html.Partial("_Product", item)

    }
